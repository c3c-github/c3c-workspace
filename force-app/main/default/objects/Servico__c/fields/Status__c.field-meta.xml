<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status__c</fullName>
    <description>Calcula se o serviço está ativo baseando-se na data de início e fim em relação a hoje.</description>
    <externalId>false</externalId>
    <formula>IF(AND(DataInicio__c &lt;= TODAY(), OR(ISBLANK(DataFim__c), DataFim__c &gt;= TODAY())), &quot;Ativo&quot;, &quot;Inativo&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Status</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>